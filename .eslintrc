{
  "extends": ["eslint:recommended", "react"],

  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-console": "off",
    "strict": [
      "error",
      "global"
    ],
    "curly": "warn",
    "jsx-quotes": ["error","prefer-single"]
  }
}